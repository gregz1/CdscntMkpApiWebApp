<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CdiscountMarketPlaceApi_WebApp</title>

    <environment names="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">Cdiscount Marketplace API WebApp</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
                </ul>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Product Manager<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("GetAllAllowedCategoryTree", "GetAllAllowedCategoryTreeRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("SubmitProductPackage", "SubmitProductPackageRequest", "ProductManager")</li>
                            <li>@Html.ActionLink("GetAllowedCategoryTree", "GetAllowedCategoryTreeRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("GetAllModelList", "GetAllModelListRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("GetModelList", "GetModelListRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("GetProductList", "GetProductListRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("GetBrandList", "GetBrandListRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("GetProductPackageSubmissionResult", "GetProductPackageSubmissionResultRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("GetProductPackageMatchingFileData", "GetProductPackageMatchingFileDataRequest", "ProductManager")</li>                            
                            <li>@Html.ActionLink("GetProductListByIdentifier", "GetProductListByIdentifierRequest", "ProductManager")</li>

                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Offer manager<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Seller management", "Index", "Seller")</li>
                            <li>@Html.ActionLink("Reactivate Seller", "ReactivateSellers", "Seller")</li>
                            <li>@Html.ActionLink("Unban Seller", "UnbanSellers", "Seller")</li>
                            <li>@Html.ActionLink("Batch role Update", "BatchRolesUpdate", "Seller")</li>
                            <li>@Html.ActionLink("CLeursIP", "Index", "SellerLogs")</li>
                            <li>@Html.ActionLink("Quotas", "Index", "Quota")</li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Order manager<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Product packages", "ProductPackages", "Package")</li>
                            <li>@Html.ActionLink("Offer packages", "Index", "OfferPackage")</li>
                            <li>@Html.ActionLink("Offers", "Index", "Offer")</li>
                            <li>@Html.ActionLink("Offer integration codes", "ErrorCodeInfo", "OfferPackage")</li>
                            <li>@Html.ActionLink("Where is my SKU", "Index", "WhereIsMySku")</li>
                            <li>@Html.ActionLink("Rights management", "Index", "ProductRights")</li>
                            <li>@Html.ActionLink("Image Check", "Index", "ImageCheck")</li>                           
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">CRM manager<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Veille", "Monitoring", "Moderation")</li>
                            <li>@Html.ActionLink("Fermeture - dépublication", "Closing", "Moderation")</li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <br />
            <br />
            <p>&copy; 2017 - CdiscountMarketPlaceApi_WebApp</p>
        </footer>
    </div>

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
